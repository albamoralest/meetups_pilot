        ont:subject         <http://dbpedia.org/resource/Peter_Dawson_(bass-baritone)> ;
        ont:timeExpression  "of 1918" .

<http://w3id.org/polifonia/pilot/meetups/442116/6/2>
        rdf:type            ont:MusicalMeetup ;
