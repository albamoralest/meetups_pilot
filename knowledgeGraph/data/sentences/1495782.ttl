        ont:place           <http://dbpedia.org/resource/Crown_Point,_Indiana> ;
        ont:subject         <http://dbpedia.org/resource/Ruth_Etting> ;
        ont:timeExpression  "July 17 , 1922" .

<http://w3id.org/polifonia/pilot/meetups/1495782/17/10>
        rdf:type            ont:MusicalMeetup ;
        ont:excerpt         "In August 1940, Myrl Alderman asked the district attorney to drop further prosecution attempts against Snyder for the 1938 shooting." ;
        ont:person          <http://dbpedia.org/resource/Alderman> , <http://dbpedia.org/resource/John_Otterbein_Snyder> , <http://dbpedia.org/resource/District_attorney> ;
        ont:subject         <http://dbpedia.org/resource/Ruth_Etting> ;
        ont:timeExpression  "August 1940" , "1938 shooting" .

<http://w3id.org/polifonia/pilot/meetups/1495782/13/0>
        rdf:type     ont:MusicalMeetup ;
        ont:excerpt  "Etting fell in love with her pianist, Myrl Alderman, who was separated from his wife." ;
        ont:person   <http://dbpedia.org/resource/Alderman> ;
        ont:subject  <http://dbpedia.org/resource/Ruth_Etting> .

<http://w3id.org/polifonia/pilot/meetups/1495782/2/5>
        rdf:type            ont:MusicalMeetup ;
        ont:excerpt         "Three days after Alderman was shot, his wife filed suit against Etting for alienation of affections." ;
        ont:person          <http://dbpedia.org/resource/Alderman> ;
        ont:subject         <http://dbpedia.org/resource/Ruth_Etting> ;
        ont:timeExpression  "Three" .

<http://w3id.org/polifonia/pilot/meetups/1495782/13/18>
        rdf:type     ont:MusicalMeetup ;
        ont:excerpt  "Upon seeing Etting's gun, Moe Snyder wrested it away from her; it landed on the floor." ;
        ont:person   <http://dbpedia.org/resource/Martin_Snyder> ;
        ont:subject  <http://dbpedia.org/resource/Ruth_Etting> .

<http://w3id.org/polifonia/pilot/meetups/1495782/15/11>
        rdf:type            ont:MusicalMeetup ;
        ont:excerpt         "Alma Alderman's lawsuit ended in December 1939, with the court finding that she was not entitled to damages from Ruth Etting." ;
        ont:person          <http://dbpedia.org/resource/Ruth_Etting> , <http://dbpedia.org/resource/Alderman> ;
        ont:subject         <http://dbpedia.org/resource/Ruth_Etting> ;
        ont:timeExpression  "December 1939" .

<http://w3id.org/polifonia/pilot/meetups/1495782/15/5>
        rdf:type     ont:MusicalMeetup ;
        ont:excerpt  "During the course of the trial, there was also a question of the validity of Alderman's marriage to Alma." ;
