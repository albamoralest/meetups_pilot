        ont:subject  <http://dbpedia.org/resource/Ruth_Etting> .

<http://w3id.org/polifonia/pilot/meetups/1495782/7/12>
        rdf:type            ont:MusicalMeetup ;
        ont:excerpt         "The three spent the night rewriting the song so Etting could perform it." ;
        ont:subject         <http://dbpedia.org/resource/Ruth_Etting> ;
        ont:timeExpression  "three" .

<http://w3id.org/polifonia/pilot/meetups/1495782/15/4>
        rdf:type     ont:MusicalMeetup ;
        ont:excerpt  "Ruth Etting testified that she was not married to Alderman." ;
        ont:person   <http://dbpedia.org/resource/Ruth_Etting> , <http://dbpedia.org/resource/Alderman> ;
        ont:subject  <http://dbpedia.org/resource/Ruth_Etting> .

