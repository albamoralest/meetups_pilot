        ont:subject         <http://dbpedia.org/resource/Heinrich_Jalowetz> ;
        ont:timeExpression  "Johanna Groag.From 1909" , "In 1908" , "1933" .

<http://w3id.org/polifonia/pilot/meetups/3411455/2/1>
        rdf:type            ont:MusicalMeetup ;
        ont:excerpt         "A musicology pupil of Guido Adler, Jalowetz was among Arnold Schoenberg's first students in Vienna, 1904–1908." ;
