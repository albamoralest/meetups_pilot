        ont:subject         <http://dbpedia.org/resource/Luther_Allison> ;
        ont:timeExpression  "in 1994" , "four" .

<http://w3id.org/polifonia/pilot/meetups/1429918/5/2>
        rdf:type            ont:MusicalMeetup ;
