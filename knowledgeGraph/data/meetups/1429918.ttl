<http://w3id.org/polifonia/pilot/meetups/1429918/2/13>
        rdf:type            ont:MusicalMeetup ;
        ont:excerpt         "He moved to France in 1977." ;
        ont:place           <http://dbpedia.org/resource/France> ;
        ont:subject         <http://dbpedia.org/resource/Luther_Allison> ;
