        ont:place    <http://dbpedia.org/resource/Harrison,_Arkansas> ;
        ont:subject  <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/0/7>
        rdf:type     ont:MusicalMeetup ;
        ont:excerpt  "He worked with Jimmy Dawkins, Magic Sam and Otis Rush, and also backed James Cotton." ;
        ont:person   <http://dbpedia.org/resource/James_Cotton> , <http://dbpedia.org/resource/Magic_Sam> , <http://dbpedia.org/resource/Jimmy_Dawkins> ;
        ont:subject  <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/3/4>
        rdf:type            ont:MusicalMeetup ;
