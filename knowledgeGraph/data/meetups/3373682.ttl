        ont:person          <http://dbpedia.org/resource/Jesus> ;
        ont:subject         <http://dbpedia.org/resource/Grahame_Bond> ;
        ont:timeExpression  "in 2006" .

<http://w3id.org/polifonia/pilot/meetups/3373682/6/2>
        rdf:type            ont:MusicalMeetup ;
